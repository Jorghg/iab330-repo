<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ff1bbe94">
    <TextView
        android:text="  Contact us"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:layout_width="match_parent"
        android:layout_height="55.5dp"
        android:id="@+id/textView1"
        android:drawableTint="#ff0f1a1b"
        android:includeFontPadding="false"
        android:textColor="#ff0d0406"
        android:typeface="sans"
        android:textStyle="bold"
        android:layout_gravity="center"
        android:textAlignment="center"
        android:gravity="center_vertical" />
    <GridLayout xmlns:tools="http://schemas.android.com/tools"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="102.5dp"
        android:id="@+id/gridLayout1"
        tools:visibility="gone" />
    <Button
        android:text="Email us"
        android:layout_width="200.0dp"
        android:layout_height="80dp"
        android:id="@+id/btnbutton1"
        android:drawableTint="#ff009688"
        android:backgroundTint="#ff00695c"
        android:textStyle="bold"
        android:layout_gravity="center"
        android:textColor="#ffffffff" />
    <GridLayout
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:id="@+id/gridLayout2" />
    <Button
        android:text="Call us"
        android:layout_width="200.0dp"
        android:layout_height="80dp"
        android:id="@+id/button2"
        android:backgroundTint="#ff00695c"
        android:textStyle="bold"
        android:layout_gravity="center"
        android:textColor="#ffffffff" />
    <GridLayout
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="24.0dp"
        android:id="@+id/gridLayout3" />
    <Button
        android:text="Find us"
        android:layout_width="200.0dp"
        android:layout_height="80dp"
        android:id="@+id/button3"
        android:backgroundTint="#ff00695c"
        android:textStyle="bold"
        android:layout_gravity="center"
        android:textColor="#ffffffff" />
    <GridLayout
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="24dp"
        android:id="@+id/gridLayout4" />
    <Button
        android:text="Find Provider"
        android:layout_width="200.0dp"
        android:layout_height="80dp"
        android:id="@+id/button4"
        android:backgroundTint="#ff00695c"
        android:textStyle="bold"
        android:layout_gravity="center"
        android:textColor="#ffffffff" />
</LinearLayout>